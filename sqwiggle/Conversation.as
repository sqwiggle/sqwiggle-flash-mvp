package sqwiggle {	import sqwiggle.Member;	import sqwiggle.Self;	import sqwiggle.GUID;	import flash.utils.Dictionary;	public class Conversation extends Object {		var id:String;		var members:Dictionary;				public function Conversation() {			this.id = GUID.create();		}						public function addMember(member:Member):void {			members[member.userId] = member;			member.audioOn();		}						public function removeMember(member:Member):void {			delete members[member.userId];			member.audioOff();		}						public function inConversation(member:Member):Boolean {			return !!members[member.userId];		}	}}