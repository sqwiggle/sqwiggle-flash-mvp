package com.sqwiggle.events {		import flash.events.Event;	public class ConversationEvent extends Event {				public var data;				public function ConversationEvent(type:String, data:Object, bubbles:Boolean=false, cancelable:Boolean=false){			super(type, bubbles, cancelable);			this.data = data;		}				/** 		 * @inheritDoc		 */		override public function clone():Event{			return new ConversationEvent(type, data, bubbles, cancelable);		}				/** 		 * @inheritDoc		 */				override public function toString():String{			return "[ConversationEvent " + type + "]";		}	}}